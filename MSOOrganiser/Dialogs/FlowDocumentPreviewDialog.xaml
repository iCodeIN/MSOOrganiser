<Window x:Class="MSOOrganiser.Dialogs.FlowDocumentPreviewDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print Preview" Height="900" Width="720">
    <Grid>
        <DockPanel>
            <StackPanel DockPanel.Dock="Bottom" Orientation="Horizontal" HorizontalAlignment="Right">
                <Button Margin="10" Width="80" Click="ok_Click">Print</Button>
                <Button Margin="10" Width="80" Click="cancel_Click">Cancel</Button>
            </StackPanel>
            <FlowDocumentPageViewer x:Name="docViewer" Zoom="50">

            </FlowDocumentPageViewer>
        </DockPanel>
    </Grid>
</Window>
